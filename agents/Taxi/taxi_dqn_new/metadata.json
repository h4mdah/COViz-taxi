{
    "env": {
        "id": "Taxi-v3",
        "max_steps": 200
    },
    "agent": {
        "model": {
            "type": "mlp",
            "in": 500,
            "out": 6,
            "layers": [
                64,
                64
            ],
            "activation": "relu"
        },
        "exploration": {
            "method": "EpsilonGreedy",
            "epsilon_min": 0.01,
            "tau": 5000
        },
        "gamma": 0.95,
        "target_update": 500,
        "optimizer": {
            "type": "Adam",
            "learning_rate": 0.001
        },
        "loss_function": "l2",
        "memory_capacity": 50000,
        "batch_size": 100,
        "device": "cuda:best",
        "double": true
    }
}